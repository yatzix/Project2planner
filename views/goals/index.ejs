<%- include('../partials/header') %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Created On</th>
      <th>Deadline</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% goals.forEach(g => { %>
        <tr>
            <td><%= g.name %></td>
            <td><%= g.created %></td>
            <td><%= g.deadline %></td>
            <td><%= g.status %></td>
        </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>
