<header>
    <img src="https://i.imgur.com/6KbaZFU.png" alt="calander logo" class="logo">
    <h1>Goal Tracker</h1>
    <h2>Your Goal's Bestie</h2>
    <nav>
        <% if(user) { %>
            <a href="/"> <button class="homeButton">Home</button> </a>
            <a href="/goals/new"> <button class="newGoalButton">New Goal</button> </a>
            <a href="/goals"> <button class="seeGoalButton">See All Goals</button> </a>
            <a href="/logout"> <button class="logout-button-header-partial">LOG OUT</button> </a>
        <% } else { %>
            <a href="/auth/google" class="login">LOG IN&nbsp;<img class="loginImage" src="https://i.imgur.com/FHjYyi0.png>"></a>
        <% } %>
    </nav>
</header>


